{
    "name": "lemmotresto/laravel-moneybird",
    "homepage": "https://github.com/lemmotresto/laravel-moneybird",
    "description": "Fork of Casdr/laravel-moneybird. Moneybird package for Laravel based on Picq'ers client",
    "license": "MIT",
    "keywords": [
        "laravel",
        "moneybird"
    ],
    "authors": [
        {
            "name": "Max Berkelmans",
            "email": "maxberkelmans@live.nl"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "picqer/moneybird-php-client": ">=0.26.0"
    },
    "autoload": {
        "psr-4": {
            "LemmoTresto\\Moneybird\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LemmoTresto\\Moneybird\\MoneybirdServiceProvider"
            ],
            "aliases": {
                "Moneybird": "LemmoTresto\\Moneybird\\MoneybirdFacade"
            }
        }
    }
}
